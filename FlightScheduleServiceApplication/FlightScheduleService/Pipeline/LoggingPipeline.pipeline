                <con1:assign xmlns:con1="http://www.bea.com/wli/sb/stages/transform/config">
                    <con1:expr>
                        <con2:xqueryText xmlns:con2="http://www.bea.com/wli/sb/stages/config">let $errorCount := fn:count($body/flig:submitScheduleFlightResponseMessage/flig:ProcessingResults/flig:ProcessingResult)
